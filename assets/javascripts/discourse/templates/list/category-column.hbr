<td class='category topic-list-data'>
  {{#if siteSettings.projects_addon}}
    {{#if category.project}}
      {{project-link category.project}}<br />
      {{category-link category}}
    {{else if category.is_project}}
      {{project-link category}}
    {{/if}}
  {{else}}
    {{category-link category}}
  {{/if}}
</td>
